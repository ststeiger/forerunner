{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": [
		"../Forerunner/Common/js/",
        "../Forerunner/ReportExplorer/js/",
        "../Forerunner/ReportViewer/js/",
		"readme.md"
	],
        "exclude": [],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
	],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "encoding": "utf8",
        "destination": "./Docs/",
        "recurse": true,
        "tutorials": "./Tutorials",
        "query": "",
        "private": true,
        "lenient": true,
		"template": "templates/forerunner"
    }
}