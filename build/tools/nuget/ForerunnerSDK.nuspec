      <frameworkAssembly assemblyName="System.Web" targetFramework="net45" />
      <frameworkAssembly assemblyName="System.Web.Extensions" targetFramework="net45" />
      <frameworkAssembly assemblyName="System.Xml" targetFramework="net45" />
