  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT        Customers.CompanyName, Customers.CustomerID, Customers.Address, Customers.City, Customers.Region, Customers.PostalCode, Customers.Phone, Orders.OrderID, Orders.OrderDate, 
                         Employees.EmployeeID, Employees.LastName, Employees.FirstName
FROM            Customers INNER JOIN
                         Orders ON Customers.CustomerID = Orders.CustomerID INNER JOIN
                         Employees ON Orders.EmployeeID = Employees.EmployeeID</CommandText>
      </Query>
