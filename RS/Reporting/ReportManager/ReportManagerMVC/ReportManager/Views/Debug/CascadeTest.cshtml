    $(document).ready(function () {
    
        // Create a reportViewerEZ widget to view the report
        var $reportViewerEZ = $('body').reportViewerEZ({
            DefaultAppTemplate: null,
            navigateTo: null,
            historyBack: null,
            isReportManager: false,
            isFullScreen: true,
        });

        var defaultParamsList = {
          "ParamsList": [{
            "Parameter": "TypeValue",
            "IsMultiple": "false",
            "Type": "String",
            "Value": "1",
            "Prompt": "Type Values"
          }]
        }
 
        var $reportViewer = $reportViewerEZ.reportViewerEZ("getReportViewer");

        $reportViewer.reportViewer("loadReport", "/Northwind Test Suite/Cascading Parameter Sticky", 1, defaultParamsList);

    });
      
  </script>

