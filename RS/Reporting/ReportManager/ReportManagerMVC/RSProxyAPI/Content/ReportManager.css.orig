html,body {
  height: 100%;
  background-color: white;
}

* {
  padding: 0px;
  margin: 0px;
  border: 0px;
  z-index: 0;
}

table,td,tr {
  padding: 0px;
  margin: 0px;
  border: 0px;
  z-index: 0;
}

header#topdiv {
  border-top: 10px solid black;
  position: fixed;
  top:0px;
  left:0px;
  width:100%;
  height: 55px;
  background-color: white;
  z-index: 1;
}

footer#bottomdiv {
  border-width: 0px;
  position: fixed;
  bottom:0px;
  left:0px;
  width:100%;
  height: 150px;
  background-color: white;
  z-index: 1;
}

#headerspacer {
  height:66px;
  display:block;
}

#footerspacer {
  height:0px;
  display:block;
}

section#mainSectionHeader {
  height: 20px;
  border-top: 1px solid black;
  border-bottom: 1px solid black;
  width:100%;
  background-color: white;
}

section#pageSection {
  display:block;
}

.overflow-hidden {
  overflow: hidden;
}

.sky-carousel {
  display: block;
}

.rm-list {
  display: none;
  width: device-width;
  background: -moz-linear-gradient(top,  rgba(0,0,0,0.65) 0%, rgba(0,0,0,0) 100%); /* FF3.6+ */
  background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(0,0,0,0.65)), color-stop(100%,rgba(0,0,0,0))); /* Chrome,Safari4+ */
  background: -webkit-linear-gradient(top,  rgba(0,0,0,0.65) 0%,rgba(0,0,0,0) 100%); /* Chrome10+,Safari5.1+ */
  background: -o-linear-gradient(top,  rgba(0,0,0,0.65) 0%,rgba(0,0,0,0) 100%); /* Opera 11.10+ */
  background: -ms-linear-gradient(top,  rgba(0,0,0,0.65) 0%,rgba(0,0,0,0) 100%); /* IE10+ */
  background: linear-gradient(to bottom,  rgba(0,0,0,0.65) 0%,rgba(0,0,0,0) 100%); /* W3C */
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#a6000000', endColorstr='#00000000',GradientType=0 ); /* IE6-9 */
}

.catalogitem {
  width: 250px;
  display: inline;
}

/* Smartphones (landscape) ----------- */
@media only screen 
and (max-height : 320px) 
and (min-width : 400px) 
and (orientation : landscape) {
/* Styles */
.catalogitem {
  width: 180px;
}

.sc-content {
  display:inline;
}

.sky-carousel {
  height: 250px;
  width: device-width;
}

.rm-list {
  display: none;
}

.spacer20mm {
  min-width:1mm;
}

.spacer10mm {
  min-width:1mm;
}

.spacer5mm {
  min-width:1mm;
}

#bottomdiv {
  display: none;
}

#footerspacer {
  display: none;
}

header#topdiv {
  border-top: 10px solid black;
  border-bottom: 1px solid black;
  position: fixed;
  top:0px;
  left:0px;
  width:100%;
  height: 35px;
  background-color: white;
  z-index: 1;
}

#headerspacer {
  height:46px;
  display:block;
}

#mainSectionHeader {
    display:none;
}
}

/* Smartphones (portrait) ----------- */
@media only screen 
and (max-width : 320px) 
and (orientation : portrait) {
/* Styles */
.catalogitem {
  width: 120px;
}

.sc-content {
  display:inline;
}

#browse-carousel {
  display: none;
}

.rm-list {
  display: block;
}

.spacer20mm {
  min-width:1mm;
}

.spacer10mm {
  min-width:1mm;
}

.spacer5mm {
  min-width:1mm;
}

#bottomdiv {
  display: none;
}

#footerspacer {
  display: none;
}

header#topdiv {
  border-top: 10px solid black;
  border-bottom: 1px solid black;
