  <Code>Public Function GetLabelTranslation(Parm as Parameter, LabelID as String, _
                Optional AddColon as Boolean=False) as String

Dim t as Integer

For t = 0 to Ubound(Parm.Value)
   If (Parm.Value(t) = LabelID) Then
      if AddColon = true then
         Return Parm.Label(t) &amp; ":"
      else
         Return Parm.Label(t)
      End if
   End if
Next t

Return ""

End Function
